{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "supabase": "npx supabase",
    "supabase:check": "node -e \"const { exec } = require('child_process'); exec('npx supabase --version', (error, stdout, stderr) => { if (error) { console.log('❌ Supabase CLI is not available:', error.message); console.log('To install, run: npm install supabase'); } else { console.log('✅ Supabase CLI is available'); console.log('Version:', stdout.trim()); } });\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/vite": "^4.1.16",
    "@types/react-router-dom": "^5.3.3",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "daisyui": "^5.3.10",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "supabase": "^2.58.5",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.9"
  }
}
